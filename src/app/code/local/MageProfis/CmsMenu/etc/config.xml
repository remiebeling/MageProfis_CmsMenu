<?xml version="1.0"?><config>    <modules>        <MageProfis_CmsMenu>            <version>0.0.1</version>        </MageProfis_CmsMenu>    </modules>    <global>        <helpers>            <cmsmenu>                <class>MageProfis_CmsMenu_Helper</class>            </cmsmenu>        </helpers>		<blocks>			<cmsmenu>				<class>MageProfis_CmsMenu_Block</class>			</cmsmenu>		</blocks>		<models>            <cmsmenu>                <class>MageProfis_CmsMenu_Model</class>            </cmsmenu>        </models>        <resources>            <mageprofiscmsmenu_setup>                <setup>                    <module>MageProfis_CmsMenu</module>                    <class>MageProfis_CmsMenu_Model_Resource_Setup</class>                </setup>            </mageprofiscmsmenu_setup>        </resources>        <events>            <adminhtml_cms_page_edit_tab_main_prepare_form>                <observers>                    <mageprofis_cmsmenu_add_order_field>                        <type>singleton</type>                        <class>cmsmenu/observer</class>                        <method>addFieldsToCmsMainForm</method>                    </mageprofis_cmsmenu_add_order_field>                </observers>            </adminhtml_cms_page_edit_tab_main_prepare_form>            <controller_action_predispatch_faq>                <observers>                    <mageprofis_faq_add_cmsmenu>                        <type>singleton</type>                        <class>cmsmenu/observer</class>                        <method>registerVirtualGroup</method>                    </mageprofis_faq_add_cmsmenu>                </observers>            </controller_action_predispatch_faq>        </events>    </global>    <adminhtml>        <acl>            <resources>                <admin>                    <children>                        <system>                            <children>                                <config>                                    <children>                                        <cmsmenu>                                            <title>Mageprofis CMS Menu</title>                                        </cmsmenu>                                    </children>                                </config>                            </children>                        </system>                    </children>                </admin>            </resources>        </acl>        <translate>            <modules>                <MageProfis_CmsMenu>                    <files>                        <default>MageProfis_CmsMenu.csv</default>                    </files>                </MageProfis_CmsMenu>            </modules>        </translate>    </adminhtml>    <frontend>	    <layout>	        <updates>	            <cmsmenu>	                <file>MageProfis_CmsMenu.xml</file>	            </cmsmenu>	        </updates>	    </layout>	</frontend></config>